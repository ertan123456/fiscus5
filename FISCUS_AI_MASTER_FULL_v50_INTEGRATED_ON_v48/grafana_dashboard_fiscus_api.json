{
  "title": "FISCUS API Overview",
  "timezone": "browser",
  "panels": [
    {
      "type": "graph",
      "title": "HTTP Requests (total)",
      "targets": [
        {
          "expr": "sum by (method, path, status) (http_requests_total)"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Latency (p95)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le,method,path))"
        }
      ]
    },
    {
      "type": "graph",
      "title": "Audit Events (rate)",
      "targets": [
        {
          "expr": "sum by (action) (rate(audit_events_total[5m]))"
        }
      ]
    }
  ],
  "schemaVersion": 39
}